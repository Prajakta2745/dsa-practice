class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:

    #to solve this exmple we use hash map method
    #time:O(n),space:O(n)
        prevMap = {} # val : index

        for i, n in enumerate(nums):
            diff = target - n
            if diff in prevMap:
                return[prevMap[diff],i]
            prevMap[n] = i
        return 

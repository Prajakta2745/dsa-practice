class Solution:
    def reverseString(self, s: List[str]) -> None:
     
        l , r = 0, len(s) - 1 #there is two pointer l ,r = for l we set o foe r we set -1
        while l < r: #two pointer are not cross each other
            s[l], s[r] = s[r], s[l] # swap the element
            l, r = l + 1, r - 1  #update the pointer
